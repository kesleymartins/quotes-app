<main class="container">

  <div class="header">
    <h1>Quotes</h1>
    <%= link_to 'New Quote', new_quote_path, data: { turbo_frame: dom_id(Quote.new) }, class: "button" %>
  </div>

  <%= turbo_frame_tag dom_id(Quote.new) %>

  <%= turbo_frame_tag :quotes do %>
    <%= render @quotes %>
  <% end %>
</main>
